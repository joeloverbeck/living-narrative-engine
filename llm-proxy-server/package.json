{
  "name": "llm-proxy-server",
  "version": "1.0.0",
  "description": "Backend proxy server for securely handling API keys for LLM services.",
  "main": "src/core/server.js",
  "type": "module",
  "scripts": {
    "start": "node src/core/server.js",
    "dev": "node --watch src/core/server.js",
    "test": "jest --env=jsdom --silent --coverage",
    "test:single": "jest --env=jsdom",
    "test:ci": "jest --env=jsdom --ci --silent --coverage",
    "format": "prettier --write \"**/*.{js,json,md,cjs}\" --log-level error",
    "format:check": "prettier --check \"**/*.{js,json,md,cjs}\" --log-level error",
    "lint": "eslint . --ext .js,.cjs --fix"
  },
  "keywords": [
    "llm",
    "proxy",
    "api-security"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.27.1",
    "@babel/preset-env": "^7.27.2",
    "@jest/globals": "^29.7.0",
    "babel-jest": "^29.7.0",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-jest": "^28.12.0",
    "eslint-plugin-jsdoc": "^50.7.1",
    "globals": "^16.2.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.0.0",
    "prettier": "^3.5.3"
  }
}
