{
  "name": "llm-proxy-server",
  "version": "1.0.0",
  "description": "Backend proxy server for securely handling API keys for LLM services.",
  "main": "src/core/server.js",
  "type": "module",
  "scripts": {
    "start": "node src/core/server.js",
    "dev": "node --watch src/core/server.js",
    "test": "jest --silent --coverage",
    "test:single": "jest --silent=false --runInBand",
    "test:ci": "jest --ci --silent --coverage",
    "typecheck": "tsc",
    "format": "prettier --write \"**/*.{js,json,md,cjs}\" --log-level error",
    "format:check": "prettier --check \"**/*.{js,json,md,cjs}\" --log-level error",
    "lint": "eslint . --ext .js,.cjs --fix"
  },
  "keywords": [
    "llm",
    "proxy",
    "api-security"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "dotenv": "^17.0.1",
    "express": "^5.1.0",
    "express-rate-limit": "^7.5.1",
    "express-validator": "^7.2.1",
    "helmet": "^8.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.27.1",
    "@babel/preset-env": "^7.27.2",
    "@eslint/js": "^9.28.0",
    "@jest/globals": "^30.0.4",
    "babel-jest": "^30.0.4",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-jest": "^29.0.1",
    "eslint-plugin-jsdoc": "^51.3.4",
    "globals": "^16.2.0",
    "jest": "^30.0.4",
    "prettier": "^3.5.3",
    "typescript": "^5.8.3"
  }
}
