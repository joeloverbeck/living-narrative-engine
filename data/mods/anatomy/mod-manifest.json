{
  "$schema": "http://example.com/schemas/mod-manifest.schema.json",
  "id": "anatomy",
  "version": "1.0.0",
  "name": "Anatomy System",
  "description": "Provides entity anatomy system with body parts, blueprints, and recipes for humanoid characters.",
  "author": "joeloverbeck",
  "dependencies": [
    {
      "id": "descriptors",
      "version": "^1.0.0"
    }
  ],
  "gameVersion": ">=0.0.1",
  "content": {
    "components": [
      "body.component.json",
      "joint.component.json",
      "part.component.json",
      "sockets.component.json"
    ],
    "entities": {
      "definitions": [
        "human_breast_d_cup.entity.json",
        "human_eye.entity.json",
        "human_eye_blue.entity.json",
        "human_eye_brown.entity.json",
        "human_eye_cobalt.entity.json",
        "human_female.entity.json",
        "human_hair_raven.entity.json",
        "human_leg_shapely.entity.json",
        "humanoid_arm.entity.json",
        "humanoid_head.entity.json",
        "humanoid_leg.entity.json",
        "human_female_torso.entity.json"
      ],
      "instances": ["jacqueline_rouxel.entity.json"]
    },
    "blueprints": [
      "humanoid_standard.blueprint.json"
    ],
    "recipes": [
      "gorgeous_milf.recipe.json",
      "humanoid_standard.recipe.json"
    ],
    "anatomyFormatting": [
      "anatomy-formatting/default.json"
    ]
  }
}
