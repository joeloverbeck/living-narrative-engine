{
  "$schema": "http://example.com/schemas/condition.schema.json",
  "id": "core:event-is-action-dismiss",
  "description": "Checks if the triggering event is for the 'core:dismiss' action.",
  "logic": {
    "==": [
      {
        "var": "event.payload.actionId"
      },
      "core:dismiss"
    ]
  }
}
