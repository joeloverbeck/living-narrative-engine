{
  "$schema": "schema://living-narrative-engine/anatomy.recipe.schema.json",
  "recipeId": "core:example_filter_socketid",
  "blueprintId": "core:example_creature_blueprint",
  "slots": {},
  "patterns": [
    {
      "matchesAll": {
        "socketId": "leg_socket_*"
      },
      "partType": "socketed_leg",
      "tags": ["anatomy:part"],
      "properties": {
        "anatomy:part": {
          "socketType": "standard",
          "attachmentStrength": "strong"
        }
      }
    },
    {
      "matchesAll": {
        "socketId": "wing_socket_*"
      },
      "partType": "socketed_wing",
      "tags": ["anatomy:part", "anatomy:membranous"],
      "properties": {
        "anatomy:part": {
          "socketType": "flexible",
          "attachmentStrength": "moderate"
        }
      }
    }
  ],
  "bodyDescriptors": {
    "build": "aerodynamic"
  }
}
