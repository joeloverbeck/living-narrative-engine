            "minimum": -1,
            "maximum": 1
          },
          "self_evaluation": {
            "type": "number",
            "minimum": -1,
            "maximum": 1
          },
          "sexual_arousal": {
            "type": "number",
            "minimum": -1,
            "maximum": 1
          },
          "sex_excitation": {
            "type": "number",
            "minimum": -1,
            "maximum": 1
