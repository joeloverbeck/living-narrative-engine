            "minimum": -1,
            "maximum": 1
          },
          "arousal": {
            "type": "number",
            "minimum": -1,
            "maximum": 1
          },
          "agency_control": {
            "type": "number",
            "minimum": -1,
            "maximum": 1
          },
          "threat": {
            "type": "number",
            "minimum": -1,
            "maximum": 1
