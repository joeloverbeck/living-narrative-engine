{
  "$schema": "schema://living-narrative-engine/condition.schema.json",
  "id": "sex-penile-oral:event-is-action-pull-penis-out-of-mouth-revulsion",
  "description": "Checks if the event is an attempt_action event for pull_penis_out_of_mouth_revulsion",
  "logic": {
    "and": [
      { "==": [{ "var": "event.type" }, "core:attempt_action"] },
      {
        "==": [
          { "var": "event.payload.actionId" },
          "sex-penile-oral:pull_penis_out_of_mouth_revulsion"
        ]
      }
    ]
  }
}
