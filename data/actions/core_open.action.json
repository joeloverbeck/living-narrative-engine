{
  "id": "core:open",
  "name": "Open",
  "description": "Allows opening entities that are currently closed and not locked.",
  "commands": [
    "open"
  ],
  "requires_target": true,
  "target_type": "entity_in_room",
  "applicability": {
    "requires_components": [
      "Openable"
    ],
    "requires_target_state": {
      "Openable.isOpen": false
    },
    "excluded_target_state": {
      "Lockable.isLocked": true
    }
  }
}