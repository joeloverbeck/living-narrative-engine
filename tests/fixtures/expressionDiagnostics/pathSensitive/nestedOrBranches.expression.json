    {
      "logic": {
        "or": [
          {
            "and": [
              { ">=": [{ "var": "emotions.joy" }, 0.5] },
              {
                "or": [
                  { ">=": [{ "var": "emotions.excitement" }, 0.3] },
                  { ">=": [{ "var": "emotions.enthusiasm" }, 0.3] }
                ]
              }
            ]
          },
          { ">=": [{ "var": "emotions.serenity" }, 0.6] }
        ]
      }
    }
  ],
  "effects": []
}
