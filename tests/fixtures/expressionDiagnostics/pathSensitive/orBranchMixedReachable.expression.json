{
  "$schema": "schema://living-narrative-engine/expression.schema.json",
  "id": "test:orBranchMixedReachable",
  "description": "Test expression where some OR branches are reachable and some have gate conflicts (entrancement has agency_control constraint)",
  "prerequisites": [
    {
      "logic": {
        "and": [
          { ">=": [{ "var": "emotions.flow" }, 0.85] },
          {
            "or": [
              { ">=": [{ "var": "emotions.interest" }, 0.45] },
              { ">=": [{ "var": "emotions.entrancement" }, 0.4] }
            ]
          }
        ]
      }
    }
  ],
  "effects": []
}
